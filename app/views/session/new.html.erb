
<%= form_with url: login_path, local: true do |f| %>


<ul class="form-style-1">
  <h2 class= "heading">Login </h2>

    <li>

      <%= f.label :email %> <br>
      <%= f.text_field :email %> <br>
    </li>
    <li>
      <%= f.label :password %> <br>
      <%= f.password_field :password %><br>
      <br> <br>

    </li>



    <%= f.submit "login" %>

</ul>



<% end %>
